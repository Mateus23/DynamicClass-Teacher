<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mateu.dynamicclass_teacher">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity" />
        <activity android:name=".NewClassActivity" />
        <activity android:name=".MySubjects" />
        <activity android:name=".SubjectActivity" />
        <activity android:name=".MyChapters" />
        <activity android:name=".ChapterActivity" />
        <activity android:name=".NewChapterActivity" />
        <activity android:name=".MyExercises" />
        <activity android:name=".NewExercise" />
        <activity android:name=".ExerciseActivity" />
        <activity android:name=".NewChapterRoutine"></activity>
    </application>

</manifest>